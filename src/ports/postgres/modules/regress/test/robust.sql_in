--------------------- Robust linear regression install checks ----------- 

SELECT * FROM (
	SELECT (robust_linregr(y, ARRAY[1, x1, x2], coef)).*
	FROM
	(		SELECT (linregr(y, ARRAY[1, x1, x2])).coef
			FROM weibull
	) as mle_coef,
	weibull as src
) q;

select robust_variance('weibull', 'result_weibull', 'linear',
                    'y', 'ARRAY[1, x1, x2]', ARRAY[-153.51, 1.24, 12.08]);

